{
  "applicationId": "neutralino.arknight",
  "defaultMode": "window",
  "port": 8081,
  "enableServer": true,
  "enableNativeAPI": true,
  "url": "/",
  "documentRoot": "/resources/",
  "tokenSecurity": "none",
  "version": "2.4",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "window.*",
    "computer.*",
    "storage.*",
    "debug.*",
    "events.*",
    "updater.*"
  ],
  "globalVariables": {
    "TEST1": "Hello",
    "TEST2": [
      2,
      4,
      5
    ],
    "TEST3": {
      "value1": 10,
      "value2": {}
    }
  },
  "modes": {
    "window": {
      "title": "Arknight-Script",
      "width": 1200,
      "height": 800,
      "minWidth": 600,
      "minHeight": 400,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/icons/appIcon.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    },
    "browser": {
      "globalVariables": {
        "TEST": "Test value browser"
      },
      "nativeBlockList": [
      ]
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    },
    "chrome": {
      "width": 1200,
      "height": 800,
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "nativeBlockList": [
      ]
    }
  },
  "cli": {
    "binaryName": "Arknight-Script",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/arknight-vue/public/neutralino.js",
    "binaryVersion": "4.5.0",
    "clientVersion": "3.4.0"
  }
}